<h2 class="line">look</h2>
<hr>
<p id="notice"><%= notice %></p>

<h1>Object list</h1>

<table>
  <thead>
    <tr>
      <th>object name</th>
      <th>leasing</th>
      <th>residence</th>
      <th>Number of years</th>
      <th>pro forma</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.rent %></td>
        <td><%= room.address %></td>
        <td><%= room.age %></td>
        <td><%= room.content %></td>
        <td><%= link_to 'Details', room %></td>
        <td><%= link_to 'Editing', edit_room_path(room) %></td>
        <td><%= link_to 'delete', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Register your property', new_room_path %>
